{
    "name": "lvovgeka/symfony-json-rpc-bundle",
    "description": "Rpc server for symfony that supports annotation of all Http methods.",
    "license": "MIT",
    "version" : "1.0.0",
    "type": "symfony-bundle",
    "keywords" : ["symfony-rpc-bundle", "rpc-bundle"],
    "authors": [
        {
            "name": "Evgeny Lvov",
            "email": "lvovgeka@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lvovgeka\\JsonRpcBundle\\": ""
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": ">=3.0",
        "doctrine/orm": ">=2.5",
        "doctrine/doctrine-bundle": ">=1.6",
        "doctrine/doctrine-cache-bundle": ">=1.2",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": ">=3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    }
}
